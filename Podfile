def shared_pods
	use_frameworks!
    pod 'Firebase/Core'
    pod 'SWXMLHash', '~> 3.0.0'
    pod 'Alamofire', '~> 4.0'
    pod 'ReachabilitySwift'
    pod 'Firebase/AdMob'
    pod 'Fabric'
    pod 'Crashlytics'

end

target 'PBike' do

    shared_pods

end

target 'CBike' do

    shared_pods

end

target ‘PBike Dev’ do

    shared_pods

end
